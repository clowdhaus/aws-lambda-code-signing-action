{
  "name": "aws-lambda-code-signing-action",
  "version": "0.0.0",
  "private": true,
  "description": "GitHub action which uses AWS Code Signer to sign ✍🏼 AWS Lambda artifacts 📦 from your pipeline",
  "keywords": [
    "github",
    "actions",
    "lambda",
    "code-signing",
    "git-ops"
  ],
  "homepage": "https://github.com/clowdhaus/aws-lambda-code-signing-action#readme",
  "bugs": {
    "url": "https://github.com/clowdhaus/aws-lambda-code-signing-action/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clowdhaus/aws-lambda-code-signing-action.git"
  },
  "license": "Apache-2.0",
  "author": "Clowd Haus, LLC",
  "main": "src/index.ts",
  "scripts": {
    "all": "yarn lint && yarn compile",
    "compile": "ncc build -m",
    "lint": "eslint . --quiet --fix",
    "release": "semantic-release"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "ci": false,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): version ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@aws-sdk/client-s3": "^3.569.0",
    "@aws-sdk/client-signer": "^3.569.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^22.13.10",
    "@vercel/ncc": "^0.38.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.22.0",
    "lint-staged": "^15.5.0",
    "semantic-release": "^24.2.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "resolutions": {
    "fast-xml-parser": ">=4.4.1"
  }
}
